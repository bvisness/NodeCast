<!DOCTYPE html>
<html>

<head>
    <title>{{ title }}</title>
    <link rel="stylesheet" href="style.css">
    <script type="text/javascript" src="/scripts/jquery-1.12.2.min.js"></script>
    <script type="text/javascript" src="/scripts/config.js"></script>
    <script type="text/javascript" src="/scripts/websocket.js"></script>
    <script type="text/javascript" src="/scripts/live.js"></script>
</head>

<body class="live">
    <div class="view live-match">
        <div class="container">
            <div class="info-container">
                <div class="logo left"></div>
                <div class="info-text match">Qualification XX of XX</div>
                <div class="info-text event">Event Name</div>
                <div class="logo right"></div>
            </div>
            <div class="match-container">
                <div class="castle left alliance-color" data-strength="8">
                    <div class="castle-strength">8</div>
                </div>
                <div class="defenses left alliance-color">
                    <div class="defense" data-strength="2"></div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense" data-strength="2"></div>
                </div>
                <div class="center-container">
                    <div class="teams left alliance-color">
                        <div class="team"></div>
                        <div class="team"></div>
                        <div class="team"></div>
                    </div>
                    <div class="timer-scores">
                        <div class="timer">
                            <div class="timer-fill"></div>
                            <div class="timer-count">0</div>
                        </div>
                        <div class="score left alliance-color">0</div>
                        <div class="score right alliance-color">0</div>
                    </div>
                    <div class="teams right alliance-color">
                        <div class="team"></div>
                        <div class="team"></div>
                        <div class="team"></div>
                    </div>
                </div>
                <div class="defenses right alliance-color">
                    <div class="defense" data-strength="2"></div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense" data-strength="2"></div>
                </div>
                <div class="castle right alliance-color" data-strength="8">
                    <div class="castle-strength">8</div>
                </div>
            </div>
        </div>
    </div>
    <div class="view rankings"></div>
    <div class="view upcoming"></div>
</body>

</html>
