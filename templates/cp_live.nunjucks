<!DOCTYPE html>
<html>

<head>
    <title>{{ title }}</title>
    <link rel="stylesheet" href="style.css">
    <script type="text/javascript" src="/scripts/jquery-1.12.2.min.js"></script>
    <script type="text/javascript" src="/scripts/config.js"></script>
    <script type="text/javascript" src="/scripts/websocket.js"></script>
    <script type="text/javascript" src="/scripts/keyboard_map.js"></script>
    <script type="text/javascript" src="/scripts/cp_live.js"></script>
</head>

<body class="cp-live">
    <div class="center-container">
        <div class="wide-container">
            <div class="match-info">
                <input class="match-key" placeholder="Match Key">
            </div>
            <div class="time-buttons">
                <div class="time-button start-auto">Start Auto</div>
                <div class="time-button start-teleop">Start Teleop</div>
                <div class="time-button cancel-match">Abort Match</div>
                <div class="time-button reset-match">Reset</div>
            </div>
            <div class="timer"></div>
            <div class="scores">
                <input class="score left red" value="0">
                <input class="score right blue" value="0">
            </div>
            <div class="teams left red">
                <input class="team">
                <input class="team">
                <input class="team">
            </div>
            <div class="teams right blue">
                <input class="team">
                <input class="team">
                <input class="team">
            </div>
        </div>

        <div class="left-container">
            <div class="defenses left red">
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
            </div>
            <div class="castle left red" data-strength="8">
                <div class="castle-strength">8</div>
                <div class="castle-btn castle-down-btn">-</div>
                <div class="castle-btn castle-up-btn">+</div>
            </div>
        </div>

        <div class="right-container">
            <div class="defenses right blue">
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
                <div class="defense-container">
                    <div class="defense-btn defense-down-btn">-</div>
                    <div class="defense" data-strength="2"></div>
                    <div class="defense-btn defense-up-btn">+</div>
                </div>
            </div>
            <div class="castle right blue" data-strength="8">
                <div class="castle-strength">8</div>
                <div class="castle-btn castle-down-btn">-</div>
                <div class="castle-btn castle-up-btn">+</div>
            </div>
        </div>
    </div>
</body>

</html>
